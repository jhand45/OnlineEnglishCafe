@model IEnumerable<OnlineEnglishCafe.Models.Teacher>

    @{
        ViewData["Title"] = "Online Lessons - ";
    }

    <h2>Online Lessons</h2>

    @*Creates a grid that will display relevant teacher details*@

    <div class="container">
    @for (var i = 0; i < Model.Count(); i++)
    {
        <div class="row">
            <div class="col-md-6">
                <p>model details</p>
            </div>
            @i++;
            <div class="col-md-6">
                <p>model +1 details</p>
            </div>
        </div>
    }
    </div>



